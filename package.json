{
  "name": "@binaryoverload/nuxt-sentry",
  "version": "0.1.2",
  "description": "Nuxt 3 module for Sentry",
  "repository": "https://github.com/binaryoverload/nuxt-sentry",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxi prepare playground && nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "prettier -w . && eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.6.5",
    "@sentry/vite-plugin": "^2.7.0",
    "@sentry/vue": "^7.64.0",
    "defu": "^6.1.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.6.5",
    "@nuxt/test-utils": "^3.6.5",
    "@types/node": "^18.17.8",
    "changelogen": "^0.5.5",
    "eslint": "^8.47.0",
    "nuxi": "^3.11.1",
    "nuxt": "^3.6.5",
    "prettier": "^3.0.2",
    "vitest": "^0.34.2"
  }
}
